[Unit]
Description="tunebank"

[Service]
ExecStart=/usr/local/bin/node /home/john/services/tunebank/tunebank.js
Restart=always
User=root
Group=nogroup
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production PORT=8080
WorkingDirectory=/home/john/services/tunebank

[Install]
WantedBy=multi-user.target

